{
  "name": "fileloader",
  "version": "1.0.0",
  "description": "more stable file loader for nunjucks, and support charsets like `gbk`.",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "mocha --check-leaks -R spec -t 5000 test/*.test.js",
    "test-cov": "node node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --check-leaks -t 5000 test/*.test.js",
    "test-travis": "node node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcovonly -- --check-leaks -t 5000 test/*.test.js",
    "jshint": "jshint .",
    "autod": "autod -w --prefix '~'",
    "cnpm": "npm install --registry=https://registry.npm.taobao.org",
    "contributors": "contributors -f plain -o AUTHORS"
  },
  "dependencies": {
    "debug": "~2.2.0",
    "iconv-lite": "~0.4.8",
    "wt": "~1.0.0"
  },
  "devDependencies": {
    "autod": "*",
    "contributors": "*",
    "istanbul": "*",
    "jshint": "*",
    "mm": "~1.1.0",
    "mocha": "*",
    "should": "~6.0.1"
  },
  "homepage": "https://github.com/node-modules/fileloader",
  "repository": {
    "type": "git",
    "url": "git://github.com/node-modules/fileloader.git",
    "web": "https://github.com/node-modules/fileloader"
  },
  "bugs": {
    "url": "https://github.com/node-modules/fileloader/issues",
    "email": "fengmk2@gmail.com"
  },
